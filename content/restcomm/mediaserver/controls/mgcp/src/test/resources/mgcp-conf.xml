<?xml version="1.0" encoding="UTF-8"?>

<mgcp>
    <!-- Implemented packages -->
    <packages>
        <package name="AU">
            <signal name="pa" handler="org.restcomm.media.control.mgcp.pkg.au.Play"/>
            <signal name="pc" handler="org.restcomm.media.control.mgcp.pkg.au.PlayCollect"/>
        </package>        
    </packages>

    <!-- Mapping packages to endpoints -->
    <endpoints>
        <endpoint local-name="mobicents/ivr/$" packages="AU"/>
    </endpoints>
</mgcp>
    